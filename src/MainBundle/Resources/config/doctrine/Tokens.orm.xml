<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MainBundle\Entity\Tokens" table="tokens">
    <unique-constraints>
      <unique-constraint name="uid" columns="uid"/>
      <unique-constraint name="token" columns="token"/>
    </unique-constraints>
    <id name="uid" type="integer" column="uid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="token" type="string" column="token" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userid" type="integer" column="userId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createtime" type="datetime" column="createTime" nullable="false"/>
  </entity>
</doctrine-mapping>
